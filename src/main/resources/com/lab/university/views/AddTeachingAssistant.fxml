<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lab.university.controllers.AddTeachingAssistantController">
   <Label layoutX="149.0" layoutY="36.0" text="Add Teaching Assistant">
      <font>
         <Font name="Times New Roman Bold Italic" size="36.0" />
      </font>
   </Label>
   <GridPane alignment="CENTER" layoutX="143.0" layoutY="99.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="270.0" prefWidth="314.0">
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="153.79998779296875" minWidth="10.0" prefWidth="132.8000244140625" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="206.60003662109375" minWidth="10.0" prefWidth="182.39997558593748" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <Label text="Name: ">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Label>
      <Label text="Gender: " GridPane.rowIndex="1">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Label>
      <Label text="Phone Number: " GridPane.rowIndex="2">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Label>
      <Label text="Address: " GridPane.rowIndex="3">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" promptText="Enter the name" GridPane.columnIndex="1" />
      <TextField fx:id="phoneNumberField" promptText="Enter the phone Numbers" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="addressField" promptText="Enter the address" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
      <Button mnemonicParsing="false" onAction="#onBack" prefHeight="33.0" prefWidth="84.0" text="Back" GridPane.rowIndex="7">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" onAction="#onAddTeacherAssistant" prefHeight="33.0" prefWidth="84.0" text="Add" GridPane.columnIndex="1" GridPane.rowIndex="7">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Button>
      <Label text="UserName: " GridPane.rowIndex="4">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="userNameField" promptText="Enter the userName" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Label text="Password: " GridPane.rowIndex="5">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="passwordField" promptText="Enter the password" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <Label text="Course: " GridPane.rowIndex="6">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="courseField" promptText="Enter the course" GridPane.columnIndex="1" GridPane.rowIndex="6" />
      <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <RadioButton fx:id="male" layoutX="14.0" layoutY="8.0" mnemonicParsing="false" onAction="#onRadioButtonToggle" selected="true" text="Male" />
         <RadioButton fx:id="female" layoutX="100.0" layoutY="7.0" mnemonicParsing="false" onAction="#onRadioButtonToggle" text="Female" />
      </AnchorPane>
   </GridPane>
</AnchorPane>
